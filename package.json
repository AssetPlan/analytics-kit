{
  "name": "@assetplan/analytics-kit",
  "version": "0.0.8",
  "description": "A complete analytics toolkit with provider wrappers and presets for consistent tracking across different analytics services",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "analytics",
    "tracking",
    "wrapper",
    "provider",
    "kit",
    "toolkit",
    "presets"
  ],
  "author": "AssetPlan",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.4",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/assetplan/analytics-kit.git"
  },
  "bugs": {
    "url": "https://github.com/assetplan/analytics-kit/issues"
  },
  "homepage": "https://github.com/assetplan/analytics-kit#readme"
}
