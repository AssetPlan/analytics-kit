{
  "name": "@assetplan/analytics-kit",
  "version": "0.0.5",
  "description": "A complete analytics toolkit with provider wrappers and presets for consistent tracking across different analytics services",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/",
    "src/",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "analytics",
    "tracking",
    "wrapper",
    "provider",
    "kit",
    "toolkit",
    "presets"
  ],
  "author": "AssetPlan",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "rollup": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/assetplan/analytics-kit.git"
  },
  "bugs": {
    "url": "https://github.com/assetplan/analytics-kit/issues"
  },
  "homepage": "https://github.com/assetplan/analytics-kit#readme"
}
